{
  "name": "londoncss-website",
  "description": "London CSS",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": "git@github.com:LondonCSS/website.git",
  "author": "Oliver Turner <oliver.turner@codedsignal.co.uk>",
  "homepage": "https://www.londoncss.dev/",
  "bugs": "https://github.com/LondonCSS/website/issues",
  "keywords": [
    "London CSS",
    "website",
    "eleventy",
    "css"
  ],
  "main": "index.js",
  "dependencies": {
    "@11ty/eleventy": "0.8.2",
    "@11ty/eleventy-plugin-rss": "1.0.6",
    "clean-css": "4.2.1",
    "date-fns": "1.30.1",
    "markdown-it": "8.4.2",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.14",
    "postcss-clean": "1.1.0",
    "postcss-cli": "6.1.2",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-pseudo-class-enter": "5.0.0",
    "workbox-cli": "4.2.0"
  },
  "devDependencies": {
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0"
  },
  "scripts": {
    "lint:css": "stylelint ./styles --config ./tools/.stylelintrc",
    "lint": "run-s lint:*",
    "css": "postcss ./styles/*.css --dir ./dist/styles",
    "dev:css": "yarn run css -w",
    "dev:serve": "eleventy --serve",
    "dev": "run-p dev:*",
    "build:css": "yarn run css",
    "build:site": "eleventy",
    "build:sw": "workbox generateSW ./tools/workbox-config.js",
    "build": "run-s build:*"
  }
}
